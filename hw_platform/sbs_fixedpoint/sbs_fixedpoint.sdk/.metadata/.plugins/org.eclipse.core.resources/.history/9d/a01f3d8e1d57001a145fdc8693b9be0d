#include "miscellaneous.h"
#include "stdio.h"

void _assert(const char * file, int line, const char * function, const char * expression)
{
  int BypassFail = 0;
  printf ("Fail: %s, in \"%s\" [%s, %d]\n", expression, function, file, line);

  while (!BypassFail)
    ;

  printf ("Bypass Fail\n");
}
